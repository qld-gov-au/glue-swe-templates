{
  "name": "Queensland-Government-Web-Template",
  "version": "3.3.0",
  "wt-version": "1.3.0",
  "description": "This template is designed to provide a template for all Franchise websites, and the underpinning technology for new Agency websites.",
  "main": "gulpfile.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/qld-gov-au/qg-web-template.git"
  },
  "scripts": {
    "clean": "gulp clean-build && gulp clean-release",
    "prebuild": "gulp clean-build",
    "build": "gulp build && node gulp/build-tasks/node-docs-flatten.js",
    "postbuild": "gulp clean-redundant-build",
    "prerelease": "gulp clean-release",
    "release": "npm run build && gulp release && node gulp/release-tasks/node-template-flatten.js",
    "postrelease": "gulp release-docs-relative-assets && gulp clean-redundant-release",
    "start": "npm run build && gulp watch",
    "watch": "gulp watch",
    "publish-wt": "gulp wt-clean && gulp wt-clone && gulp wt-sync && gulp wt-updateVersion && gulp wt-npm && gulp wt-commit && gulp wt-push",
    "publish-cdn": "gulp cdn-clean && gulp cdn-clone && gulp cdn-sync && gulp cdn-commit && gulp cdn-push",
    "prepublish": "npm run release",
    "publish": "npm run publish-wt && npm run publish-cdn",
    "postpublish": "gulp clean-publish"
  },
  "authors": "Queensland Government",
  "licence": "all rights reserved",
  "homepage": "http://www.qld.gov.au/",
